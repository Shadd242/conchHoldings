<UserControl x:Class="forexRootKitEA.Controls.Orders"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dc="clr-namespace:forexRootKitEA.Controls"
             mc:Ignorable="d">
    <TabControl>
        <TabItem Header="Pivot Point">
            <dc:PivotPoint HorizontalAlignment="Center" x:Name="pivotPoint"/>
        </TabItem>
        <TabItem Header="Moving Average">
            <dc:MA x:Name="mA"/>
        </TabItem>
        <TabItem Header="Stop Loss">
            <dc:StopLoss x:Name="stopLoss"/>
        </TabItem>
        <TabItem Header="Take Profit">
            <dc:TakeProfit x:Name="takeProfit"/>
        </TabItem>
        <TabItem Header="Pip Step">
            <dc:PipStep x:Name="pipStep"/>
        </TabItem>
        <TabItem Header="Empty Order Mode">
            <dc:EOM x:Name="emptyOrderMode"/>
        </TabItem>
    </TabControl>
</UserControl>
